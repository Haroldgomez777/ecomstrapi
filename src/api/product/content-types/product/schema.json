{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "availableForSale": {
      "type": "boolean"
    },
    "description": {
      "type": "text"
    },
    "descriptionHtml": {
      "type": "text"
    },
    "product_variants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-variant.product-variant"
    },
    "tags": {
      "type": "json"
    },
    "options": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-option.product-option"
    },
    "priceRange": {
      "type": "component",
      "repeatable": true,
      "component": "price-range.price-range"
    },
    "variants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-variant.product-variant"
    },
    "seo": {
      "type": "component",
      "repeatable": true,
      "component": "seo.seo"
    },
    "handle": {
      "type": "string"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
